<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/black"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btnFechar"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:layout_marginTop="48dp"
        android:text="Fechar"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/btnProx"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnAnterior"
        app:layout_constraintTop_toBottomOf="@+id/txtNome2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/peralta"
        android:text="Listar usÃºarios."
        android:textColor="@color/white"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="226dp"
        android:layout_height="211dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="20dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/listagem_removebg_preview" />
    </LinearLayout>

    <TextView
        android:id="@+id/txtStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/allerta_stencil"
        android:text="Registros :"
        android:textColor="@color/white"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/txtTelefone"
        android:layout_width="130dp"
        android:layout_height="57dp"
        android:background="@color/white"
        android:gravity="center"
        android:textColor="@color/white"
        android:textColorHint="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="335dp"
        tools:layout_editor_absoluteY="381dp" />

    <TextView
        android:id="@+id/txtNome2"
        android:layout_width="125dp"
        android:layout_height="56dp"
        android:layout_marginTop="12dp"
        android:background="@color/white"
        android:gravity="center"
        android:textColor="@color/white"
        android:textColorHint="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toStartOf="@+id/txtTelefone"
        app:layout_constraintHorizontal_bias="0.425"
        app:layout_constraintStart_toEndOf="@+id/txtEndereco2"
        app:layout_constraintTop_toBottomOf="@+id/txtStatus" />

    <TextView
        android:id="@+id/txtEndereco2"
        android:layout_width="130dp"
        android:layout_height="56dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:background="@color/white"
        android:gravity="center"
        android:textColor="@color/white"
        android:textColorHint="#000000"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtStatus" />

    <Button
        android:id="@+id/btnAnterior"
        android:layout_width="148dp"
        android:layout_height="79dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="48dp"
        android:text="Anterior"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEndereco2" />

    <Button
        android:id="@+id/btnProx"
        android:layout_width="154dp"
        android:layout_height="80dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="16dp"
        android:text="Proximo"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTelefone" />

</androidx.constraintlayout.widget.ConstraintLayout>